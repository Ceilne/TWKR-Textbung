;COMMENT	アイテムを追加した場合は、CASEに追記
;-----------------------------------------------------------
@PHARMACY_RANK(ARG:0)
#FUNCTION
SELECTCASE ARG:0
	CASE 40, 45, 58, 213, 216, 500, 507, 510, 641, 642
		RETURNF 1
	CASE 42, 59, 195, 215, 501, 502, 509, 850, 851
		RETURNF 2
	CASE 41, 250, 252, 503, 506, 508, 513
		RETURNF 3
	CASE 71, 72, 91, 505, 511, 515, 516, 852 
	;, 853
		RETURNF 4
	CASE 74, 77, 78, 512, 514
		RETURNF 5
ENDSELECT
