    단백질 = 26
    지방 = 4
    나트륨 = 49 
    비타민 = 30
ELSEIF ARG == GETNUM(ITEM,"당근")
    ;당근
    탄수화물 = 10
    단백질 = 1
    지방 = 0
    나트륨 =69 
    비타민 = 20
ELSEIF ARG == GETNUM(ITEM,"순무")
