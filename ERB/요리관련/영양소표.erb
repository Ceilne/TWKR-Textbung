    ;콩 33g 쌀 33g 보리 33g
    탄수화물 = 41
    단백질 = 17
    지방 = 7
    나트륨 = 5
    비타민 = 15
ELSEIF ARG == GETNUM(ITEM,"묵은쌀")||ARG == GETNUM(ITEM,"백미")
