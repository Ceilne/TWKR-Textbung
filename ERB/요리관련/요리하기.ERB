    CALL 요리재료표시함수(표시되는재료:LOCAL)
    IF RESULT == 1
        LOCAL:1++
    ENDIF
    IF LOCAL:1 == 4
        PRINTL
        LOCAL:1 = 0
    ENDIF
NEXT
