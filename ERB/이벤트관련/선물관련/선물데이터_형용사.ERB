VARSET RESULTS
VARSET RESULT
;分布
SIF INRANGE(NOW_AREA, 0, 10) && GETBIT(SELL_AREA, NOW_AREA)
	RESULT:0 = 1
;옷감品は광물・기술がかからない
SIF STRFIND(SENSE_MAIN, "옷감") >= 0 && STRFIND(SENSE, "광물") + STRFIND(SENSE, "기술") >= -1
	RESULT:0 = 0
;철제品は섬유がかからない
SIF STRFIND(SENSE_MAIN, "철제") >= 0 && STRFIND(SENSE, "섬유") >= 0
	RESULT:0 = 0
;초상物は광물、섬유、기술がかからない
SIF STRFIND(SENSE_MAIN, "초상") >= 0 && STRFIND(SENSE, "광물") + STRFIND(SENSE, "섬유") + STRFIND(SENSE, "기술") >= -2
	RESULT:0 = 0
;향수専用
SIF STRFIND(SENSE_MAIN, "향수") >= 0 && STRFIND(SENSE, "광물") + STRFIND(SENSE, "섬유") + STRFIND(SENSE, "기술") + STRFIND(SENSE, "모양") >= -3
	RESULT:0 = 0
;生体品は식물がかからない
SIF STRFIND(SENSE_MAIN, "생체품") >= 0 && STRFIND(SENSE, "식물") >= 0
	RESULT:0 = 0

SENSE += NAME_ADJECT + "/"

RESULT:1 = MAGNIF_PRICE
RESULT:2 = SELL_AREA
RESULTS:0 = %NAME_ADJECT%
RESULTS:1 = %SENSE%
RETURN RESULT:0
