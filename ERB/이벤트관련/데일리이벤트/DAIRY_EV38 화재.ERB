
@DAIRY_CONDITION_EV38
SIF TALENT:MASTER:연령 != 0
	RETURN -1
SIF TALENT:MASTER:성별 != 2
	RETURN -1
SIF RAND:100 > 20
	RETURN -1
CALL DAIRY_EV38
