	{
	CALL HPH_PRINT,@"「%TEXTR("んっ/あっ/あんっ/フーーッ")%HPH %TEXTR("んっ/あっ/あんっ/フーーッ")%HPH 
	ひんっ %TEXTR("んっ/あっ/あんっ/フーーッ")%HPH んはぁっHPH」","W"
