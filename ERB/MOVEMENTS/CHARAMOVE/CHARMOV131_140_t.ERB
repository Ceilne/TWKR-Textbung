;美宵
@CHARAMOVE_DATA_134(ARG,ARG:2)
LOCAL = 134
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 수면시간(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;私室には入らない
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;鯢呑亭(仕事があるので10までいかない）
			CASE 238
				RETURN 5
			;広場
			CASE 202
				RETURN 5
			;酒屋, 식료품점, 야채가게
			CASE 211, 216, 227
				RETURN 10
			;自室はあまり居ない
			CASE 239
				RETURN -5
			;あなたの部屋
			CASE 230
				IF TALENT:LOCAL:연모
					RETURN 5
				ELSE
					RETURN -100
				ENDIF
			;他のお店とかは行かない
			CASE 212, 213, 217, 218, 220, 221, 222, 223, 224, 225, 226, 228, 229, 231 TO 237
				RETURN -100
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 1, 2
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1, 2, 3, 4, 11, 12, 13, 14, 21, 22, 23
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

