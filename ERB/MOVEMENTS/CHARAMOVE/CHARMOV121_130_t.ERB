;나루미
@CHARAMOVE_DATA_121(ARG,ARG:2)
LOCAL = 121
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;마리사寝室,마리사風呂,마리사倉庫
			CASE 512, 514, 515
				RETURN -100
			;앨리스私室,神綺私室,夢子私室,앨리스風呂,앨리스倉庫
			CASE 521, 522, 523, 526, 28
				RETURN -100
			;에렌私室,에렌倉庫,에렌風呂
			CASE 535, 536, 537
				RETURN -100
			;各棲み処
			CASE 541, 542, 543
				RETURN -100
			;특히 이루어
			CASEELSE
				RETURN 0
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 51, 52, 53, 54, 56, 57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

;마이
@CHARAMOVE_DATA_122(ARG,ARG:2)
LOCAL = 122
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
		;연동 처리(사토노가 높을 때는 사토노를 참조)
		IF TALENT:123:연모 || (TALENT:123:사모 && !TALENT:123:사모)
			CFLAG:LOCAL:현재위치 = CFLAG:123:만난위치 ?  CFLAG:123:만난위치 # SUKIMA()
		ELSE
			CFLAG:LOCAL:현재위치 = CFLAG:122:만난위치 ?  CFLAG:122:만난위치 # SUKIMA()
		ENDIF
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;특히 이루어
			CASEELSE
				RETURN 0
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 4, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 21, 22, 23, 41, 42, 43, 46, 47, 51, 52, 53, 56, 57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

