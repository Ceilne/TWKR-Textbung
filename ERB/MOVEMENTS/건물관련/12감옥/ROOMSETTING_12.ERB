@ROOMSETTING_12
VARSET ROOMDATA
FOR LOCAL, 1,100
	SIF GROUPMATCH(LOCAL, 1)
		ROOMDATA:LOCAL |= 장소_이불
	SIF INRANGE(LOCAL, 1,13)
		ROOMDATA:LOCAL |= 장소_옥내
	SIF GROUPMATCH(LOCAL, 1)
		ROOMDATA:LOCAL |= 장소_목욕탕
	SIF GROUPMATCH(LOCAL, 7)
		ROOMDATA:LOCAL |= 장소_주방
	SIF GROUPMATCH(LOCAL, 8)
		ROOMDATA:LOCAL |= 장소_훈련
	SIF GROUPMATCH(LOCAL, 1)
		ROOMDATA:LOCAL |= 장소_공부
	SIF GROUPMATCH(LOCAL, 1)
		ROOMDATA:LOCAL |= 장소_휴식
	; SIF GROUPMATCH(LOCAL, )
	; 	ROOMDATA:LOCAL |= 장소_창공
	; SIF GROUPMATCH(LOCAL, )
	; 	ROOMDATA:LOCAL |= 장소_달구경
	; SIF GROUPMATCH(LOCAL, )
	; 	ROOMDATA:LOCAL |= 장소_채소밭
	SIF !GROUPMATCH(LOCAL, 7,8,9)
		ROOMDATA:LOCAL |= 장소_좁다
	SIF GROUPMATCH(LOCAL, 1)
		ROOMDATA:LOCAL |= 장소_선잠_가능
	SIF GROUPMATCH(LOCAL, 10,11,12)
		ROOMDATA:LOCAL |= 장소_머물지않는다
	SIF GROUPMATCH(LOCAL, 13)
		ROOMDATA:LOCAL |= 장소_신체측정
	SIF !GROUPMATCH(LOCAL, 7,8,9)
		ROOMDATA:LOCAL |= 장소_과소
	SIF GROUPMATCH(LOCAL, 1)
		ROOMDATA:LOCAL |= 장소_Private
	SIF GROUPMATCH(LOCAL, 7)
		ROOMDATA:LOCAL |= 장소_거실
NEXT

