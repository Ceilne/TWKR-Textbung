;지점 관련성 응답 처리
;-----------------------------------------------------------
@CAN_MOVE_12(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	;아나타 감방/감방 1그룹
	CASE 1201 TO 1203
		SIF GROUPMATCH(ARG:1, 1210)
			RETURNF 1
	;감방 2그룹
	CASE 1204 TO 1206
		SIF GROUPMATCH(ARG:1, 1211)
			RETURNF 3
	;식당/체육실/노역실
	CASE 1207 TO 1209
		SIF GROUPMATCH(ARG:1, 1210)
			RETURNF 1
	;안쪽복도, 감방 1그룹/바깥복도 직통, 식당/체육실/노역실 연결
	CASE 1210
		SIF INRANGE(ARG:1, 1201, 1203)
			RETURNF 3
        SIF GROUPMATCH(ARG:1, 1211)
            RETURNF 3
        SIF INRANGE(ARG:1, 1207, 1209)
            RETURNF 1
    ;바깥복도, 감방 2그룹/안쪽복도 직통, 입출구/교도소장 방 연결
    CASE 1211
        SIF INRANGE(ARG:1, 1204, 1206)
            RETURNF 3
        SIF GROUPMATCH(ARG:1, 1210)
            RETURNF 3
        SIF GROUPMATCH(ARG:1, 1212,1213)
            RETURNF 1
	;입출구
	CASE 1212
		SIF GROUPMATCH(ARG:1, 1306)
			RETURNF 1
        SIF ARG:1 == MAXROOM
            RETURNF 2
    ;교도소장의 방
    CASE 1213
        SIF GROUPMATCH(ARG:1, 1211)
            RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == 1212
			RETURNF 1
ENDSELECT
RETURNF 0
