@GETMAP_2(ARG)
KEHAI = 0

FOR LOCAL, 1, CHARANUM
	IF CFLAG:LOCAL:현재위치 == ARG
		KEHAI = CHECK_HEHAI(LOCAL)
		SIF KEHAI > 1
			BREAK
	ENDIF
	IF ARG == 213 
	CALL GETMAP_BUILDING(LOCAL,213,226)
	ELSEIF ARG == 214
	CALL GETMAP_BUILDING(LOCAL,230,237)
	ENDIF
	SIF KEHAI > 1
		BREAK
NEXT
CALL SET_KEHAI_COLOR(KEHAI)
