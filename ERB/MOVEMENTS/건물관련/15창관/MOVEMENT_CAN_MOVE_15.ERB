;지점 관련성 응답 처리
;-----------------------------------------------------------
@CAN_MOVE_15(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	;카운터
	CASE 1501
		SIF GROUPMATCH(ARG:1, 1502,1504)
			RETURNF 1
	;직원실
	CASE 1502
		SIF GROUPMATCH(ARG:1, 1501,1504)
			RETURNF 1
	;대연회장
	CASE 1504
		SIF GROUPMATCH(ARG:1, 1501,1502,1503,1506,1507)
			RETURNF 1
	;;글로리홀
	;CASE 1505
    ;    SIF GROUPMATCH(ARG:1, 1504)
    ;        RETURNF 1
	;바
	CASE 1506
		SIF GROUPMATCH(ARG:1, 1504,1507)
			RETURNF 1
	;직원실2
	CASE 1507
		SIF GROUPMATCH(ARG:1, 1504,1506)
			RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == 1507
			RETURNF 1
ENDSELECT
RETURNF 0
