@M_KOJO_MESSAGE_COM_K30_0_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	;시간중
	IF FLAG:70
		RETURN 1
	ENDIF
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「우우… 손놀림… 추잡하다…」		
		RETURN 1
	ENDIF
	;마구로 스윗치 온
	IF CFLAG:1501 || TCVAR:30:억지로 || TCVAR:30:만취
		CALL K30_GASPING_MAGURO
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;삽입중
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	;V삽입
	IF LOCAL:1 && TEQUIP:50 ==  PLAYER
		;사정했다
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「응읏…… 아…… 아개… 이……」
				CASE 1
					PRINTFORMW 「앗…… 우우우……… 앗…… 나와… 루」
				CASE 2
					PRINTFORMW 「후왓… 나와… 루……… 우… 읏」
			ENDSELECT
			PRINTFORMDW 사정의 쾌감에 몸을 진동시키면서, 매끌매끌한 피부를 즐겼다.
		ELSEIF NOWEX:0 || NOWEX:1 || NOWEX:2 || NOWEX:3
			PRINTFORMDW 연결되면서의 애무에 높일 수 있어 에이키는 용이하게절정 했다…
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「아앗…… 아앗…… 굉장하다… 이……」
				CASE 1
					PRINTFORMW 「히, 아아, 읏…… 후왓」
				CASE 2
					PRINTFORMW 「하… 핫…… 후왓…… 아아!
			ENDSELECT
		ENDIF
	;A삽입
	ELSEIF LOCAL:1 && TEQUIP:51 ==  PLAYER
		;사정했다
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT(" 「응읏…… 두껍다아……… 아아앙HPH」", "W")
				CASE 1
					CALL HPH_PRINT(" 「앗………… 가득 HPH…… 나오고 있는 HPHHPH」", "W")
				CASE 2
					CALL HPH_PRINT(" 「후와아아앗…… 나와… 루… 우우우…… 응HPH」", "W")
			ENDSELECT
			PRINTFORMDW 사정의 쾌감에 몸을 진동시키면서, 매끌매끌한 피부를 즐겼다.
		ELSEIF NOWEX:0 || NOWEX:1 || NOWEX:2 || NOWEX:3
			PRINTFORMDW 연결되면서의 애무에 높일 수 있어 에이키는 용이하게절정 했다…
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「아앗…… 아앗…… 굉장하다… 이…%UNICODE(0x2665)%」
				CASE 1
					PRINTFORMW 「히, 아아, 응%UNICODE(0x2665)%… 후왓」
				CASE 2
					PRINTFORMW 「는%UNICODE(0x2665)%… 핫…… 후왓…… 아아!
			ENDSELECT
		ENDIF
	;자위 해 받은 직후
	ELSEIF PREVCOM ==  80
		PRINTFORMD 답례라는 듯이, 에이키의
		SIF PALAM:윤활 >=  PALAMLV:4
			PRINTFORMD 흠뻑에 젖었다
		PRINTFORMDL 비부[秘部]를 차분히 애무했다…
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「하… 핫…… 후왓…… 서로… 기분 좋게……%UNICODE(0x2665)%」
			CASE 1
				PRINTFORMW 「두 사람, 함께…%UNICODE(0x2665)%…… 아앗, 후와아」
			CASE 2
				PRINTFORMW 「아읏, 아아… 후훗, 답례 고마워요♪」
		ENDSELECT
	;이것으로절정 하는&랜덤 1/2
	ELSEIF (NOWEX:C절정 || NOWEX:V절정 || NOWEX:A절정 || NOWEX:B절정) && RAND:2 ==  0
	;키스도 했다
		IF EXP_UP(27, TARGET) && RAND:2 ==  0
			IF RAND:2 ==  0
				PRINTFORML 「… 응무, 날름……, 응응…! ………, 아앗. 안돼%UNICODE(0x2665)%」 
				PRINTFORMDW 에이키는 쾌감이 견디지 않고, 입을 떼어 놓아 등을 활 휘고 해 허덕인다…
			ELSE
				PRINTFORML 「츗…… 쥬푸, 읏응! …… 쥬루! … 츗……%UNICODE(0x2665)%」
				PRINTFORMDW 에이키는 쾌락의 고조됨을 입으로 옮김으로 전하도록(듯이), 뜨겁고 격렬한 키스를 계속한다…
			ENDIF
		ELSE
			IF RAND:2 ==  0
				PRINTFORML 「핫…… 후왓………! 안돼%UNICODE(0x2665)%간닷!」 
				PRINTFORMDW 에이키는 쾌감이 견디지 않고, 등을 활 휘고 해 허덕인다…
			ELSE
				PRINTFORML 「아아, 하우,……… 앗! …… 가는…… 가 버린다! %UNICODE(0x2665)%」
				PRINTFORMDW 에이키는 당신의 손가락에 희롱해져 쾌락의 높은 곳에 끝까지 올라 간다…
			ENDIF
		ENDIF
	ELSE
	;키스도 했다
		IF EXP_UP(27, TARGET) && RAND:2 ==  0
			IF RAND:2 ==  0
				PRINTFORML 「츗……%UNICODE(0x2665)%……… 후훗……… 다시 한번♪……%UNICODE(0x2665)%」
				PRINTFORMW 쪼아먹는 것 같은 키스를 즐기면서, 에이키의 매끈매끈피부를 즐겼다…
			ELSE
				PRINTFORML 「%UNICODE(0x2665)%………… 후훗, 이렇게 하고 있는 것… 좋아… 응, 츄……」
				PRINTFORMDW 애무와 키스로 릴렉스 한 에이키는 넋을 잃은 표정을 띄웠다.
			ENDIF
		ELSE
			SELECTCASE RAND:4
				CASE 0
					PRINTFORMDL 문득 생각나 에이키의 피부를 어루만지면서 목덜미에 키스를 했다.
					PRINTFORMW 「… 꺗, 그런, 들이마시면……… 안됏, 자국이 나 버린다………」
				CASE 1
					PRINTFORMW 「아쿳, 응우…… 당신에…… 접할 수 있는 것이…… 기분이 좋다…」
				CASE 2
					PRINTFORMW 「아읏……… 앗, 거기…… 좀 더……… 어루만져…… 는 %UNICODE(0x2665)%」
				CASE 3
					PRINTFORMDL 옆클리나 겨드랑 밑도 함께 상냥하게 애무했다.
					PRINTFORMW 「꺗,……… 아우…… 당신의 손, 기분이 좋다…」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
