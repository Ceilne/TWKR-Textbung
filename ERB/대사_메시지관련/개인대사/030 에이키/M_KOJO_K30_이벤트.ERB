	;오늘 만나지 않았다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;오늘 만나고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		RETURN 1
	ENDIF
ENDIF
RETURN 1

