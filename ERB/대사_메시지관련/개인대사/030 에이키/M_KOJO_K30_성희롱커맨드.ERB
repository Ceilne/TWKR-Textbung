@M_KOJO_MESSAGE_COM_K30_330_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CFLAG:우후후 == 2
		IF TEQUIP:TARGET:50 == MASTER
			IF RAND:2 == 1
				PRINTFORMW 「응%UNICODE(0x2665)%…, 보이겠지요……」
				PRINTFORMW 「나의, 여기가… 당신의 것을, 확실히 삼키고 있는 것…%UNICODE(0x2665)%」
			ELSE
				PRINTFORMW 「핫, 하앗…%UNICODE(0x2665)% 좀 더 봐, 가지고 싶은거야…」
				PRINTFORMW 「기분 좋은 곳 %UNICODE(0x2665)%들어가 있는 것, 봐 %UNICODE(0x2665)%」
			ENDIF
		ELSEIF ! EQUIP:하반신속옷2
			IF PALAM:윤활 < PALAMLV:7
				PRINTFORMW 「어때? 흥분해 왔을까……?」
			ELSE
				PRINTFORMW 「보이겠죠…… 나의, 여기」
				PRINTFORMW 「이렇게… 미끈미끈 해… 추잡해져…%UNICODE(0x2665)%」
			ENDIF
		ELSE
			IF CFLAG:1700 != 0 && RAND:2 == 1
				PRINTFORMW 「나의%PANTSNAME(EQUIP:하반신속옷2, TARGET)%, 지금은 볼 뿐이야…」
				PRINTFORMW 「갖고 싶어도, 참아」
			;관계없지만, 랜덤으로 이따금 터무니 없는 녀석은 있네요
			ELSE
				PRINTFORMW 「이%PANTSNAME(EQUIP:하반신속옷2, TARGET)%…」
				PRINTFORMW 「나에게, 어울리고 있습니까…%UNICODE(0x2665)%」
			ENDIF
		ENDIF
	ELSE
		SELECTCASE TFLAG:193
			;귀싸대기
			CASE -5
				PRINTFORMW 「……장난치고 있습니까, 당신」
				CALL K30_LECTURE_CHECK(30 + RAND:10, 1)
			;꼬집을 수 있다
			CASE -3
				PRINTFORMW 「……아이입니까, 당신」
				CALL K30_LECTURE_CHECK(25 + RAND:15, 1)
			;싫어한다
			CASE -1
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「겹쳐 쌓은 선행도…… 잃는 것은 일순간이에요?」
					CASEELSE
						PRINTFORMW 「욕망이 향하는 대로 사는 것은 반드시 후회해요」
				ENDSELECT
				CALL K30_LECTURE_CHECK(25 + RAND:10, 1)
			;싫어한다(희 무성하고)
			CASE 0
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「사람이 싫어하는 것을 진행해 준다는 의미, 잘못하고 있지 않습니까?」
					CASEELSE
						PRINTFORMW 「거절도 없고 이런 일 되면, 누구라도 싫어요?」
				ENDSELECT
			;성공
			CASE 1
				IF K30_BE_SEEN() == 0
					IF ABL:노출벽 > 3
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「부끄럽지만… 당신에라면……」
							CASEELSE
								PRINTFORMW 「보일 뿐(만큼)……%K30_C_NAME(MASTER)%에 보일 뿐(만큼)이니까…」
						ENDSELECT
					ELSE
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「비유해, 단 둘에서도…… 부끄러운, 입니다…」
							CASEELSE
								PRINTFORMW 「아……! 우, 우우, 부끄럽다……」
						ENDSELECT
					ENDIF
				ELSE
					IF ABL:노출벽 > 5
						TCVAR:30:374 = 1
						;팬티는 있다
						IF TEQUIP:0 > 1
							PRINTFORMW 「당신의 부탁인걸… 비유하고 이런 일에서도, 당신에라면…%UNICODE(0x2665)%」
						;노 팬츠에서도 괜찮아? 그리고 걷어올림
						ELSE
							IF RAND:2 == 1
								PRINTFORMW 「%K30_C_NAME(MASTER)%가 봐 준다면, 나는 뭐든지…%UNICODE(0x2665)%」
							ELSE
								PRINTFORMW 「나, 보여져…… 이런 모습을 볼 수 있어… 나……%UNICODE(0x2665)%」
							ENDIF
						ENDIF
						SIF (ABL:노출벽 > 8 || (TEQUIP:0 > 1 && ABL:노출벽 > 5)) && RAND:2 == 1
							PRINTFORMW 「……%K30_C_NAME(MASTER)%…… 다음은, 어떻게 합니까…%UNICODE(0x2665)%」
					ELSE
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「염라의… 위엄에 관련되므로… 더 이상은…」
							CASEELSE
								PRINTFORMW 「……안됩니다, ……%K30_C_NAME(K30_FIND_AROUND(), 2)%… 보고 있을테니까…」
						ENDSELECT
					ENDIF
				ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
