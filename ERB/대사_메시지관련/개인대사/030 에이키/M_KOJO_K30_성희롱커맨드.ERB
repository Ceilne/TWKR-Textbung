@M_KOJO_MESSAGE_COM_K30_312_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CFLAG:우후후 == 2
		IF TALENT:연모
			SELECTCASE TCVAR:20
				;전부터 껴안는다
				CASE 32
					IF RAND:2 == 1
						PRINTFORMW 「%K30_C_NAME(MASTER)%, 좀 더… 강하고, 꽉 해……%UNICODE(0x2665)%」
						PRINTFORMW 「츗… 읏, 츗…… 츄푸, 츄루……」
					ELSE
						PRINTFORMW 「츗……%UNICODE(0x2665)%…… 츄…… 읏」
						PRINTFORMW 「이제(벌써) 참을 수 있지 않습니까 ……나도, 그렇습니다%UNICODE(0x2665)%」
					ENDIF
				;자위
				CASE 50
					IF RAND:2 == 1
						PRINTFORMW 「%UNICODE(0x2665)%…… 츄…… 읏」
						PRINTFORMW 「이제(벌써), 손이 부재중이 되어 버려요?」
					ELSE
						PRINTFORML 「츄푸… 츄우, 응… 응츄… 아, 하아」
						PRINTFORMW 「후훗, 키스 해도 알아요?」
						PRINTFORMW 「나의 손으로 움찔움찔은 되어 있는 것…%UNICODE(0x2665)%」
					ENDIF
				;키스
				CASE 60
					IF RAND:2 == 1
						PRINTFORMW 「저기…%K30_C_NAME(MASTER)%, 좀 더… 혀를, 이고라고…… 짜는 %UNICODE(0x2665)%」
						PRINTFORMW 「응풋… 읏, 츗…… 쥬루, 츄루……」
					ELSE
						PRINTFORML 「이제(벌써), 그렇게 서두르지 않는 것… 나부터, 해 줄테니까」
						PRINTFORMW 「이봐요, 당신은 눈을 감아…… 츄…… 응%UNICODE(0x2665)%」
					ENDIF
				;85=V삽입하는 88=허리 흔들기 V
				CASE 85, 88
					IF RAND:2 == 1
						PRINTFORMW 「응풋… 아읏, 츄우우… 읏…… 아아앙%UNICODE(0x2665)%」
						PRINTFORMW 「이봐요, 혀도… 나에게, 넣어%UNICODE(0x2665)%짠다… 츄루」
					ELSE
						PRINTFORMW 「응응%UNICODE(0x2665)%……%UNICODE(0x2665)%… 짜는 %UNICODE(0x2665)%」
					ENDIF
				;87=A삽입하는 89=허리 흔들기 A
				CASE 87, 89
					IF RAND:2 == 1
						CALL HPH_PRINT(" 「응응… 응HPH… 츄루, 읏…… 응응HPH… 푸하아」", "L")
						CALL HPH_PRINT(" 「엉덩이… 입도, 굉장히, 좋은 것 HPH…」", "L")
						CALL HPH_PRINT(" 「어느 쪽도, 좀 더 HPH 좀 더 HPH 해 HPH」", "W")
					ELSE
						CALL HPH_PRINT(" 「HPH…… 날름, HPH…… 응응~HPH응HPHHPH」", "W")
					ENDIF
				;정말좋아 hold
				CASE 86
					SELECTCASE BASE:PLAYER:2
						CASE 0 TO 2999
							PRINTFORMW 「츄우우… 읏…%UNICODE(0x2665)%…… 날름, 쥬루……」
							PRINTFORMW 「응응%UNICODE(0x2665)%천천히, 즐깁시다……%UNICODE(0x2665)%」
						CASE 3000 TO 9999
							PRINTFORML 「츗…… 츄, 날름, 응응%UNICODE(0x2665)%…… 츗」
							PRINTFORMW 「안 돼요, 당신은 참아… 츗…… 츄, 날름, %UNICODE(0x2665)%」
							PRINTFORMW 「나를, 기분 좋게, 하는 것%UNICODE(0x2665)%」
						CASEELSE
							PRINTFORML 「츄루, 쥬루… 쥬루… 쥬풋, %UNICODE(0x2665)%」
							PRINTFORMW 「좋아요, 와…%UNICODE(0x2665)% 나에게, 전부… 이고라고%UNICODE(0x2665)%」
					ENDSELECT
				;무엇이 할당해질까 잘 모르기 때문에 적당하게 재사용의 츄파음
				CASEELSE
					PRINTDATAW
						DATAFORM 「츄, 츄우…… 츗…… 츄파…… 츄루」
						DATAFORM 「읏… 쥿……… 츗…… 날름, 츄우우」
						DATAFORM 「츄, 츄우…… 읏………… 츄파…… 츄루」
					ENDDATA
			ENDSELECT
		ENDIF
	ELSEIF CFLAG:수면 || TCVAR:만취 == 1
	ELSEIF CFLAG:동침중
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「응─,… 응………… 응후후♪」
			CASE 1
				PRINTFORMW 「응… 고마워요……」
				PRINTFORMW 「행복한…… 째…… 볼 수 있을 것 같, 구나」
			CASE 2
				PRINTFORMW 「읏………%UNICODE(0x2665)%」
				PRINTFORMW 「안 돼요, 눈이… 깨어 버릴 것 같다……」 
		ENDSELECT
	ELSEIF CFLAG:318 == 1 
		PRINTFORMW 「!!」
		PRINTFORMDW 에이키는 회개봉으로 %CALLNAME:MASTER%의 안면을 고정시켜두었다.
		PRINTFORMDW 엉망진창 아프다!!
	ELSEIF TFLAG:193 == 99
		IF	RAND:2 == 0
			PRINTFORMW 「…그런 일 허락한다고 생각한 것입니까?」
		ELSE
			PRINTFORMW 「발정난 개입니까? 당신은」
		ENDIF
	ELSE
		SELECTCASE CFLAG:1602
			CASE 0, 1
				IF TALENT:연모 && ABL:욕망 >=  5
				;「%CALLNAME:TARGET%는 MASTER의 입술을 비틀어 열어 혀를 걸어 왔다…」
					;대성공
					IF TFLAG:193 == 1
						PRINTFORMDL %CALLNAME:MASTER%도 탐내듯이 에이키와 혀를 서로 걸었다
						PRINTFORML 「츄우, %UNICODE(0x2665)%…… 응구…… 읏……%UNICODE(0x2665)%」
						PRINTFORMDW 에이키로부터 입술을 요구하고 떼어 놓아 주지 않는다…
					ELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「츄, 츄우…… 츗…… 츄파…… 푸핫」
								PRINTFORML 「후훗, 좀 더 어떻습니까%UNICODE(0x2665)%…… 츗… 츗」
								PRINTFORMDW 이번은 에이키로부터 눈꺼풀이나 코끝에까지, 키스를 서비스해 주었다…
								
							CASE 1
								PRINTFORML 「읏… 쥿……… 츗…… 푸핫」
								PRINTFORMDW 간신히 에이키가 입술을 떼어 놓으면, 타액의 실이 두 명의 사이를 탔다…
							CASE 2
								PRINTFORML 「쿠츗, 읏후, 응뭇…… 읏…%UNICODE(0x2665)%」
								PRINTFORMDW %CALLNAME:MASTER%도 지지 않으려고 에이키의 이빨의 뒤편까지 혀로 훑었다.
						ENDSELECT
					ENDIF
				ELSEIF ABL:욕망 >=  5
				;「%CALLNAME:TARGET%는 MASTER의 입술을 받아들였다…」
					;대성공
					IF TFLAG:193 == 1
						PRINTFORML 「츗………… 응…… 응응
						PRINTFORML 「………… 응………… 푸핫……… 하아… 하아…」
						PRINTFORMDW 키스가 끝난 뒤도 에이키는 상기 한 얼굴로 정신나가고 있다…
					ELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「츄,…… 읏……… 츄…… 하아」
								PRINTFORMW 「………… 답례군요, 츗」
								PRINTFORMDW 에이키는 %CALLNAME:MASTER%에 키스 해 돌려주었다.
							CASE 1
								PRINTFORML 「츄, 츄우…… 읏………… 츄파……」
								PRINTFORMW 「……키스는, 이렇게… 멋진…」
							CASE 2
								PRINTFORML 「응뭇,…… 무………… 읏…」
								PRINTFORMDW 에이키로부터 적극적으로 입술을 강압해 왔다…
						ENDSELECT
