;==================================================
;44, 바이브
;파과 캔슬 말 일하지 않기 때문에 여기만 덧붙여 씀
;==================================================
@M_KOJO_MESSAGE_COM_K30_44
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K30_44_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K30_44_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	;시간중·수면중
	IF CFLAG:MASTER:장난 ==  2 || FLAG:70
		RETURN 1
	ENDIF
	IF LOCAL:1 && TALENT:처녀
		IF CFLAG:1801 ==  1
			IF TCVAR:파과 != 0
				PRINTFORMW 「안됩니다! 절대로 안돼! 그런 도구로는 장난치고 있어!?」
				PRINTFORMW 「…… 진심, 이야? … 기다려…… 아니, 부탁… 싫엇!」
				PRINTFORMW 「그만두엇! … 아아아앗!! ……」
				TCVAR:30:기다리지않다 = 1
				TCVAR:30:억지로 = 1
				;플래그에 남긴다
				SIF TCVAR:파과 ==  1
					SETBIT CFLAG:1800, 2
				RETURN 1
			ELSE
				PRINTFORMW 「무엇, 그 도구…… 힛! … 안에서…… 떨린다……」
				RETURN 1
			ENDIF
		ELSE
			IF TCVAR:파과 != 0
				PRINTFORMW 「나는… 도구에 처음을……… 아니요 아무것도 아닙니다……」
				RETURN 1
			ELSE
				PRINTFORMW 「무엇, 그 도구…… 힛! … 안에서…… 떨린다……」
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	;마구로 스윗치 온
	IF CFLAG:1501 || TCVAR:30:억지로 || TCVAR:30:만취
		CALL K30_GASPING_MAGURO
		RETURN 1
	ENDIF
	;기본 세트
	;장착
	IF TEQUIP:바이브
		;연모
		IF TALENT:연모
			PRINTFORMW 
			RETURN 1
		;굴복 3
		ELSEIF MARK:괘씸각인 ==  3
			PRINTFORMW 
			RETURN 1
		;그 이외
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;해제
	ELSE
		PRINTFORMW 
	ENDIF
ENDIF
RETURN 1

