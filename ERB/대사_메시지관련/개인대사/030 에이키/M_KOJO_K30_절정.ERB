			CALL HPH_PRINT(" 「안됏, 가 버린다! 째 가서는… 아우우HPH」", "W")
		;강절정 C
		ELSEIF NOWEX:C절정 == 2
			PRINTFORMW 「아%UNICODE(0x2665)%콩이… 간닷, 응읏!」
		;절정 C
		ELSE
			PRINTFORMW 「후앗…… 앗! ……… 아… 아…%UNICODE(0x2665)%」
		ENDIF
	ENDIF
	IF NOWEX:V절정
		;초강절정 V
		IF NOWEX:V절정 == 3
			CALL HPH_PRINT(" 「두는 HPH 고향, 안됏! 앗, 응응, 응 아HPH」", "W")
		;강절정 V
		ELSEIF NOWEX:V절정 == 2
			PRINTFORMW 「아앗! (안)중 스치고 %UNICODE(0x2665)%… 간닷, 간다아아!」
		;절정 V
		ELSE
			PRINTFORMW 「하아, 하앗…… 응응%UNICODE(0x2665)%」
		ENDIF
	ENDIF
	IF NOWEX:A절정
		;초강절정 A
		IF NOWEX:A절정 == 3
			CALL HPH_PRINT(" 「HPH가…… 기분 좋다 HPH」", "L")
			CALL HPH_PRINT(" 「아HPH아HPHHPH…… 간닷, 가고 해 HPH」", "W")
		;강절정 A
		ELSEIF NOWEX:A절정 == 2
			CALL HPH_PRINT(" 「아HPH아는 HPH 안돼, 엉덩이로, 가 버리는 우우 HPH」", "W")
		;절정 A
		ELSE
			CALL HPH_PRINT(" 「아HPH…… 안됏…… 엉덩이는……… 아 HPH」", "W")
		ENDIF
	ENDIF
	IF NOWEX:B절정
		;강절정 B
		IF NOWEX:B절정 == 3
			PRINTFORMW CALL HPH_PRINT(" 「젖꼭지, 찌릿찌릿 해! 아니, 이먹는 HPH」", "W")
		;강절정 B
		ELSEIF NOWEX:B절정 == 2
			PRINTFORMW 「젖가슴 기분 좋은 것 %UNICODE(0x2665)%간닷, 가 버린다아!」
		;절정 B
		ELSE
			PRINTFORMW 「후아아아…… 아앗! ……… 하아, 하앗……%UNICODE(0x2665)%」
		ENDIF
	ENDIF
		
	IF NOWEX:M절정
		SELECTCASE TEQUIP:30:입접촉부위
		;강도가 아니고 접촉 부위에서 배분
			CASE 1
			;페라 
				PRINTFORMW 「쥬푸…, 즈즛……!! … 응읏,… 응…%UNICODE(0x2665)%」
				PRINTFORMW (…… 자지… 빨고 있는 것만으로, 가 버린…%UNICODE(0x2665)%)
			CASE 2
			;쿤닐링구스시킨다 
			CASE 4
			;키스 
				PRINTFORMW 「츗, 응츄……… 읏 ……응응%UNICODE(0x2665)%」
				PRINTFORMDW 에이키의 입술이 부르릇 떨려, 관련된 혀로부터 힘이 빠진다. 키스로 가 버린 것 같다……
			CASE 7
			;애널 봉사＠어떤 회면이다 이것 
				PRINTFORMW 「날름, 날름………!! ……%UNICODE(0x2665)%」
		ENDSELECT
	ENDIF
ENDIF
