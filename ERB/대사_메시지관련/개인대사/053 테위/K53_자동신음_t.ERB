;*********************************************************************************
;K53_PLIST_NUM			허덕여소리 리스트수 허덕여소리 패턴을 추가하면(자) 이 수를 만진다
;K53_PVOICE_FRAG			사용 가능한 허덕여소리 리스트		(=0 사용 불가　=1 사용가능
;		0:대상 부위　1:당신 부위　2:쾌감　3:고통　4:반감　5:키스　6:펠라　7:한숨　8:이라마치오　9:소리 인내
;
;K53_대사사용가능 대사계는 사용 1회까지		(=0 사용 불가　=1 사용가능
;RAND:K53_대사발생확률
;*********************************************************************************
@K53_AUTO_PVOICE

;초기화·초기설정
VARSET LOCALS

K53_NUM = 40
K53_대사발생확률 = 3

;사용 허덕여 리스트 초기화
FOR LOCAL, 0, K53_PLIST_NUM
	K53_PVOICE_FRAG:LOCAL = 0
NEXT
K53_대사사용가능 = 1


;*********************************************************************************
;합계 쾌감량, 쾌감으로부터 접촉부위를 판정
;총PALAMLV와 비치지만 아마 사용하지 않을지도
;*********************************************************************************
;쾌감 총량 판정
K53_쾌감 = CUP:쾌C + CUP:쾌V + CUP:쾌A + CUP:쾌B + CUP:쾌M

;쾌감이 주어지고 있는 부위의 총PALAMLV와
;쾌감이 주어지고 있는 부위를 검색
;검색하는 쾌감0:쾌C 1:쾌V 2:쾌A 3:쾌B 4:쾌M
K53_총PALAMLV = 0
K53_접촉부위 = 
FOR LOCAL, 0, 5
	;쾌감이 주어지고 있다
	IF CUP:TARGET:LOCAL > 0
		;총PALAMLV 증가
		K53_총PALAMLV += GETPALAMLV(PALAM:TARGET:LOCAL, 4)
		
		;접촉부위 기억
		SELECTCASE LOCAL
			CASE 0
				K53_접촉부위 += "C"
			CASE 1
				K53_접촉부위 += "V"
			CASE 2
				K53_접촉부위 += "A"
			CASE 3
				K53_접촉부위 += "B"
			CASE 4
				K53_접촉부위 += "M"
		ENDSELECT

	ENDIF
	
NEXT
;*********************************************************************************
;사용 가능 허덕여소리 리스트 판정
;*********************************************************************************
;이라마치오중
IF SELECTCOM == 140
	;이마라치오
	K53_PVOICE_FRAG:8 = 1
	IF STRCOUNT(K53_접촉부위, "C") || STRCOUNT(K53_접촉부위, "V") || STRCOUNT(K53_접촉부위, "A") || STRCOUNT(K53_접촉부위, "B")
		;헐떡이는 소리
		IF K53_자극LV("반감") >= 1
			;반감
			K53_PVOICE_FRAG:4 = 1
		;강렬한 쾌감
		ELSEIF K53_자극LV("쾌감") >= 2
			;입 막아 부위 쾌감
			K53_PVOICE_FRAG = 1
			K53_PVOICE_FRAG:2 = 1
			K53_PVOICE_FRAG:9 = 1
		ENDIF
	ELSE
		;이라마치오혐
		IF K53_자극LV("반감") >= 1
			;반감
			K53_PVOICE_FRAG:4 = 1
		ENDIF
	ENDIF
ELSEIF STRCOUNT(K53_접촉부위, "M")
	;펠라중
	IF TEQUIP:TARGET:입접촉부위 == 1
		;M 이외의접촉부위가 있다
		IF STRCOUNT(K53_접촉부위, "C") || STRCOUNT(K53_접촉부위, "V") || STRCOUNT(K53_접촉부위, "A") || STRCOUNT(K53_접촉부위, "B")
			;강렬한 쾌감
			IF K53_자극LV("쾌감") >= 2
				;쾌감이 강한 경우구 막아 강쾌감으로 대용, 펠라 허덕여 없음
				K53_PVOICE_FRAG:2 = 1
				K53_PVOICE_FRAG:9 = 1
			ELSE
				;펠라 쾌감 부위
				K53_PVOICE_FRAG = 1
				K53_PVOICE_FRAG:1 = 1
				K53_PVOICE_FRAG:2 = 1
				K53_PVOICE_FRAG:6 = 1
			ENDIF
			
		;펠라계만
		ELSE
			;펠라
			K53_PVOICE_FRAG:1 = 1
			K53_PVOICE_FRAG:6 = 1
		ENDIF
		
	;키스 or애무 or계속키스중의 행위
	ELSE
		;M 이외의접촉부위가 있다
		IF STRCOUNT(K53_접촉부위, "C") || STRCOUNT(K53_접촉부위, "V") || STRCOUNT(K53_접촉부위, "A") || STRCOUNT(K53_접촉부위, "B")
			;츄파
			;강렬한 쾌감의 경우구사기로 대용
			IF K53_자극LV("쾌감") >= 2
				K53_PVOICE_FRAG:9 = 1
			ELSE
				K53_PVOICE_FRAG:5 = 1
			ENDIF
			;키스중에 반발이 붙을 수도 있는 커맨드 실행
			IF K53_자극LV("고통") >= 1 || K53_자극LV("반감") >= 1
				;계속키스중에 손가락 넣고 처녀막 상실, 자궁구 고문등
				
				IF K53_자극LV("고통") >= 1
					;고통
					K53_PVOICE_FRAG = 1
					K53_PVOICE_FRAG:1 = 1
					K53_PVOICE_FRAG:3 = 1
				ELSE
					;반감
					K53_PVOICE_FRAG = 1
					K53_PVOICE_FRAG:1 = 1
					K53_PVOICE_FRAG:4 = 1
				ENDIF
				
			;키스＋허덕여
			ELSE
				;츄파 쾌감 부위
				K53_PVOICE_FRAG = 1
				K53_PVOICE_FRAG:1 = 1
				K53_PVOICE_FRAG:2 = 1
			ENDIF
			
		;키스
		ELSE
			;키스혐 이 분기는 발생 하지 않을 지도 모르다
			IF K53_자극LV("반감") >= 1
				;츄파 반감
				K53_PVOICE_FRAG:4 = 1
				K53_PVOICE_FRAG:5 = 1
				
			;키스, 계속키스
			ELSE
				;츄파
				K53_PVOICE_FRAG:5 = 1
			ENDIF
		ENDIF
	ENDIF

;봉사계 봉사 쾌락 판정
;기승위, 역강간계도 포함된다
ELSEIF TCVAR:108 == 1
	;허덕여소리만
	IF K53_자극LV("쾌감") >= 2
		;쾌감 부위
		K53_PVOICE_FRAG = 1
		K53_PVOICE_FRAG:2 = 1
		
	;한숨＋허덕여
	ELSEIF STRCOUNT(K53_접촉부위, "C") || STRCOUNT(K53_접촉부위, "V") || STRCOUNT(K53_접촉부위, "A") || STRCOUNT(K53_접촉부위, "B")
		;봉사 쾌감
		K53_PVOICE_FRAG = 1
		K53_PVOICE_FRAG:1 = 1
		K53_PVOICE_FRAG:2 = 1
		K53_PVOICE_FRAG:7 = 1
		
	;한숨만
	ELSE
		;한숨
		K53_PVOICE_FRAG:1 = 1
		K53_PVOICE_FRAG:7 = 1
	ENDIF
;
ELSE
	;고통
	;아픔 기준 처녀:1250 자궁구 고문:~500 스팬킹:450+유치 350 애널애무:100 정상위애널:800
	IF K53_자극LV("고통") >= 1
		;쾌감도 강하다
		IF K53_자극LV("쾌감") >= 2
			;고통 쾌감
			K53_PVOICE_FRAG = 1
			K53_PVOICE_FRAG:1 = 1
			K53_PVOICE_FRAG:2 = 1
			K53_PVOICE_FRAG:3 = 1
			
		;비명만
		ELSE
			;고통 부위
			K53_PVOICE_FRAG = 1
			K53_PVOICE_FRAG:1 = 1
			K53_PVOICE_FRAG:3 = 1
		ENDIF
		
	;반감·공포
	;반감 기준 후배위:600 삽입 G 스폿·자궁구 고문:500+정조 관념 1000
	ELSEIF K53_자극LV("반감") >= 1 || K53_자극LV("공포") >= 1
		;쾌감도 강한 삽입 G 스폿 등	2000 정도?
		IF K53_자극LV("쾌감") >= 2
			;반감 부위 쾌감
			K53_PVOICE_FRAG = 1
			K53_PVOICE_FRAG:1 = 1
			K53_PVOICE_FRAG:2 = 1
			K53_PVOICE_FRAG:4 = 1
			
		;비명만
		ELSE
			;반감 당신 부위　
			K53_PVOICE_FRAG = 1
			K53_PVOICE_FRAG:1 = 1
			K53_PVOICE_FRAG:4 = 1
		ENDIF
		
	;쾌감
	ELSEIF STRCOUNT(K53_접촉부위, "C") || STRCOUNT(K53_접촉부위, "V") || STRCOUNT(K53_접촉부위, "A") || STRCOUNT(K53_접촉부위, "B")
		;쾌감 부위 당신 부위
		K53_PVOICE_FRAG = 1
		K53_PVOICE_FRAG:1 = 1
		K53_PVOICE_FRAG:2 = 1
	
	;그 외
	ELSE
		;약계·도구 미사용의 아무것도 하지 않는, 등
		;한숨
		K53_PVOICE_FRAG:7 = 1
	ENDIF
ENDIF

;수정
;수간중
IF CFLAG:수면
	;대사계 없음
	K53_PVOICE_FRAG = 0
	K53_PVOICE_FRAG:1 = 0
	
	;고통, 공포, 반감 없음
	K53_PVOICE_FRAG:3 = 0
	K53_PVOICE_FRAG:4 = 0
	
	;쾌감을 한숨에
	IF STRCOUNT(K53_접촉부위, "C") || STRCOUNT(K53_접촉부위, "V") || STRCOUNT(K53_접촉부위, "A") || STRCOUNT(K53_접촉부위, "B")
		K53_PVOICE_FRAG:2 = 0
		K53_PVOICE_FRAG:7 = 1
	ENDIF

ELSE
	[SKIPSTART]
	;허덕여소리 인내(만들어 내기 지문이 없기 때문에, 잘 모르게 되어 있다
	
	;M 이외의접촉부위가 있다
	IF STRCOUNT(K53_접촉부위, "C") || STRCOUNT(K53_접촉부위, "V") || STRCOUNT(K53_접촉부위, "A") || STRCOUNT(K53_접촉부위, "B")
		;소질
		IF TALENT:자존심 || TALENT:자제심 || TALENT:정조 || TALENT:수치심 || TALENT:쾌감응답 == -1
			K53_PVOICE_FRAG:9 = 1
			;확립으로 완전 참을 수
			SELECTCASE RAND:4
				CASE 0
					K53_PVOICE_FRAG:2 = 0
			ENDSELECT
		;상황
		;침대 룸에서 후배위
		ELSEIF BEDROOM() && TEQUIP:체위 == 2
			K53_PVOICE_FRAG:9 = 1
			;확립으로 완전 참을 수
			SELECTCASE RAND:4
				CASE 0
					K53_PVOICE_FRAG:2 = 0
			ENDSELECT
		ENDIF
	ENDIF
	[SKIPEND]
ENDIF


;밀어넘어뜨려지고
IF CFLAG:우후후 == 2

ENDIF
;*********************************************************************************
;K53_부위대사 설정
;괴롭힘 당하는이라고 있는 부위에의 대사
;*********************************************************************************
K53_부위대사 =
IF STRCOUNT(K53_접촉부위, "C")
	IF TEQUIP:클리캡
		SELECTCASE RAND:2
			CASE 0
				K53_부위대사 = 크, 클리가, 부들거렷 , 시, 싫어
			CASE 1
				K53_부위대사 = 크, 클리토리스
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 0
				K53_부위대사 = 클리
			CASE 1
				K53_부위대사 = 거기
			CASE 2
				K53_부위대사 = 콩알
		ENDSELECT
	ENDIF
ELSEIF STRCOUNT(K53_접촉부위, "V")
	IF TEQUIP:바이브
		K53_부위대사 = 시, 싫, 배, 안에서…, …윙윙거려…
	ELSE
		SELECTCASE RAND:3
			CASE 0
				K53_부위대사 = 가랑이
			CASE 1
				K53_부위대사 = 안
			CASE 2
				K53_부위대사 = 거기
		ENDSELECT
	ENDIF
ELSEIF STRCOUNT(K53_접촉부위, "A")
	IF TEQUIP:애널바이브
		K53_부위대사 = 웃, 후우 , 엉덩이, , …,…부들부들, 떨려어…
	ELSE
		SELECTCASE RAND:3
			CASE 0
				K53_부위대사 = 엉덩이 구멍
			CASE 1
				K53_부위대사 = 똥구멍
			CASE 2
				K53_부위대사 = 엉덩이
		ENDSELECT
	ENDIF
ELSEIF STRCOUNT(K53_접촉부위, "B")
	SELECTCASE RAND:3
		CASE 0
			K53_부위대사 = 가슴
		CASE 1
			K53_부위대사 = 젖가슴
		CASE 2
			K53_부위대사 = 젖꼭지
	ENDSELECT
ELSE
	K53_PVOICE_FRAG = 0
ENDIF

;*********************************************************************************
;커맨드 대사 설정
;커맨드에 대한 대사
;*********************************************************************************
K53_커맨드대사 =
SELECTCASE SELECTCOM
	;애무
	CASE 0
		IF TEQUIP:유두캡 || TEQUIP:클리캡
			SELECTCASE RAND:2
				CASE 0
					K53_커맨드대사 = 끼우지, 맛… , 그런거…, 끼우지 , 아아아앙∼~~
				CASE 1
					K53_커맨드대사 = 읏, 티, 튕기지 마아, 캡이랑, 같이 , 히앗
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 0
					K53_커맨드대사 = 얏, 거기…동글동글 , 만지작…
				CASE 1
					K53_커맨드대사 = 이런 거, 딱히, 기분 좋지…아나……읏
				CASE 2
					K53_커맨드대사 = 손가락…, 거기 뿐, 끄, 끈질겨∼…
			ENDSELECT
		ENDIF
	;쿠니
	CASE 1
		IF TCVAR:102
			K53_커맨드대사 = 다, 당신이 더럽힌거니까, 제대로 깨끗하게 해 줘…
		ELSE
			SELECTCASE RAND:2
				CASE 0
					K53_커맨드대사 = 할짝 할짝
				CASE 1
					K53_커맨드대사 = 혀
					SELECTCASE RAND:2
						CASE 0
							K53_커맨드대사 += "로 쿡쿡"
						CASE 1
							K53_커맨드대사 += "로 할짝할짝"
					ENDSELECT
			ENDSELECT
		ENDIF
	;손가락 넣고
	CASE 3
		IF EX:질내정액 > 300
			K53_커맨드대사 = 정액…%K53_하트부여()%안에, 질척질척해…
		ELSE
			IF TEQUIP:바이브
				K53_커맨드대사 = 아, 안돼 , 거긴, 부짝부짝 , 하는 것, 아니, 야∼~~
			ELSE
				SELECTCASE RAND:2
					CASE 0
						K53_커맨드대사 = 손가락
					CASE 1
						K53_커맨드대사 = 손가락자지
				ENDSELECT
			ENDIF
		ENDIF
	;애널빨기
	CASE 4
		SELECTCASE RAND:2
			CASE 0
				K53_커맨드대사 = 할짝할짝
			CASE 1
				K53_커맨드대사 = 싫엇, 거긴, 더러, 더러운…
		ENDSELECT
	;애널애무
	CASE 5
		IF TEQUIP:애널바이브
			K53_커맨드대사 = 흣, 앗, 끄, 끌어당기지 ,맛, 그거, 움직이지, 마아, 그, 그만…
		ELSE
			SELECTCASE RAND:3
				CASE 0
					K53_커맨드대사 = 얕은 곳…, 엉덩이에… , 이상한, 기분…읏
				CASE 1
					K53_커맨드대사 = 엉덩이 꿈틀꿈틀, 히, 힘이이… 안들어가…
				CASE 2
					K53_커맨드대사 = 얏 , 소, 손가락, 뺄, 때마닷, 움찔움찔, 거렷…
			ENDSELECT
		ENDIF
	;가슴애무계
	CASE 6, 7, 12
		IF TEQUIP:유두캡
			K53_커맨드대사 = 얏, 젖꼭지, 부들부들 시러, 그만, 같이 하는 것, 싫어…
		ELSE
			SELECTCASE RAND:2
				CASE 0
					K53_커맨드대사 = 젖꼭지, 만져지는거 …, 시러 …
				CASE 1
					K53_커맨드대사 = 시, 싫어 …, 젖꼭지, 튕기지 마…
			ENDSELECT
		ENDIF
	;유두빨기
	CASE 11
		SELECTCASE RAND:3
			CASE 0
				K53_커맨드대사 = 우우…, 유두, 쯉쯉거려…, 야앗…
			CASE 1
				K53_커맨드대사 =햣!? 깨, 깨물지 마! , 살짝 무는거, 금지…잇
			CASE 2
				K53_커맨드대사 = 이, 이봐, 쯉쯉, 들이마시지… 마아…
		ENDSELECT
	;음부·애널개장
	CASE 8, 13
		;리스트 조작:반감 닫는 한숨 개방
		K53_PVOICE_FRAG:4 = 0
		K53_PVOICE_FRAG:7 = 1
		;질내사정되었다
		IF TCVAR:102
			K53_커맨드대사 = 아아~, 로리콘 자지에 더럽혀져버렸다, 스승님에게 말해버릴까나∼
		;ELSEIF TCVAR:104
		ELSE
			K53_커맨드대사 = …자, 이걸로 됬지? ……변태
		ENDIF
	;자위
	CASE 9
