						CASE 1
							LOCALS += "…"
						CASEELSE
							LOCALS += K53_하트부여()
					ENDSELECT
				ELSE
					SELECTCASE RAND:5
						CASE 0
							LOCALS += "!"
						CASE 1
							LOCALS += "!"
						CASEELSE
							LOCALS += K53_하트부여()
					ENDSELECT
				ENDIF
			ENDIF
