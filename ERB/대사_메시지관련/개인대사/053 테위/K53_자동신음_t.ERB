						CASE 6
							LOCALS += "핫"
						CASE 7
							LOCALS += "콜록"
						CASE 8
							LOCALS += "우읍"	
					ENDSELECT
					SELECTCASE RAND:3
						CASE 0
							LOCALS += "…"
						CASE 1
							LOCALS += "……"
						CASE 2
							LOCALS += K53_하트부여()
					ENDSELECT
				ENDIF
			ENDIF
		;한숨
		CASE 7
			IF K53_PVOICE_FRAG:7
				IF GETPALAMLV(PALAM:욕정, 15) >= 4
					SELECTCASE RAND:9
						CASE 0
