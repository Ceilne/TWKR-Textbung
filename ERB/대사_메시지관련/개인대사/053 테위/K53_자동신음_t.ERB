								SELECTCASE RAND:3
									CASE 0
										LOCALS += "시, 싫어…"
									CASE 1
										LOCALS += "이젠 싫어…"
									CASE 2
										LOCALS += "이거싫엇…"
								ENDSELECT
							ENDIF
						K53_대사사용가능 = 0
					ENDSELECT
					
				ENDIF
			ENDIF
		;키스
		CASE 5
