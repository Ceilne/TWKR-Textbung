@V_COM800
#DIM DYNAMIC 가커맨드개행1
;가상조교시간++
;대사를 입력하세요
PRINTFORML %마스터는% 카운터에 전화를 걸었다...
$카운터전화
DRAWLINE
PRINTFORML 무얼 주문할까?
PRINTFORML 소지금 : {MONEY}
;약물표시
DRAWLINE
PRINTFORML 약물
DRAWLINE
FOR LOCAL, 100, 199
    TRYCALLFORM D_COM{LOCAL}_ABLE
    IF 가커맨드개행1 == 4
        가커맨드개행1 = 0
        PRINTL
    ENDIF
NEXT
DRAWLINE
PRINTFORML 음식
DRAWLINE
FOR LOCAL, 200, 299
    TRYCALLFORM D_COM{LOCAL}_ABLE
    IF 가커맨드개행1 == 4
        가커맨드개행1 = 0
        PRINTL
    ENDIF
NEXT
DRAWLINE
PRINTFORML 서비스
DRAWLINE
FOR LOCAL, 300, 500
    TRYCALLFORM D_COM{LOCAL}_ABLE
    IF 가커맨드개행1 == 4
        가커맨드개행1 = 0
        PRINTL
    ENDIF
NEXT
PRINTL
;서비스표시
DRAWLINE
PRINTFORML 서비스
DRAWLINE
PRINTFORML [500] 1시간 연장(5000\\)
DRAWLINE
PRINTFORML [998]설명
PRINTFORML [999]돌아간다
INPUT
IF RESULT == 998
    $서비스설명루프
    DRAWLINE
    PRINTFORML 약물
    DRAWLINE
    FOR LOCAL, 100, 199
        TRYCALLFORM D_COM{LOCAL}_ABLE
        IF 가커맨드개행1 == 4
            가커맨드개행1 = 0
            PRINTL
        ENDIF
    NEXT
    DRAWLINE
    PRINTFORML 음식
    DRAWLINE
    FOR LOCAL, 200, 299
        TRYCALLFORM D_COM{LOCAL}_ABLE
        IF 가커맨드개행1 == 4
            가커맨드개행1 = 0
            PRINTL
        ENDIF
    NEXT
    DRAWLINE
    PRINTFORML 서비스
    DRAWLINE
    FOR LOCAL, 300, 500
        TRYCALLFORM D_COM{LOCAL}_ABLE
        IF 가커맨드개행1 == 4
            가커맨드개행1 = 0
            PRINTL
        ENDIF
    NEXT
    PRINTL
    PRINTFORML [999]돌아간다
    INPUT
    IF RESULT == 999
        GOTO 카운터전화
    ELSE
        DRAWLINE
        TRYCALLFORM D_HELP{RESULT}
        DRAWLINE
    ENDIF
    GOTO 서비스설명루프
ELSEIF RESULT == 999
    RESETCOLOR
    RETURN 1
ELSE
    TRYCALLFORM D_COM{RESULT}
    RESETCOLOR
ENDIF
GOTO 카운터전화
RETURN 1

