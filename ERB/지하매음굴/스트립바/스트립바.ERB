@이동판단(ARG,길)
;반환값 = 0-동 1-서 2-남 3-북
#DIM 길
#DIM DYNAMIC 이동가능수
#DIM DYNAMIC 동
#DIM DYNAMIC 서
#DIM DYNAMIC 남
#DIM DYNAMIC 북
IF GETBIT(길, 0) == 1
    동 = 1
    이동가능수 ++
ENDIF
IF GETBIT(길, 1) == 1
    북 = 1
    이동가능수++
ENDIF
IF GETBIT(길, 2) == 1
    서 = 1
    이동가능수++
ENDIF
IF GETBIT(길, 3) == 1
    남 = 1
    이동가능수++
ENDIF
IF CP:ARG:1 + 1 == LASTCP:ARG:0
    ;동쪽에서 옴
    IF 이동가능수 < 2
        RETURN 0
    ELSE
        SELECTCASE RAND:3
            CASE 0
                IF 북 == 1
                    RETURN 3
                ELSEIF 남 == 1
                    RETURN 2
                ELSEIF 서 == 1
                    RETURN 1
                ENDIF
            CASE 1
                IF 남 == 1
                    RETURN 2
                ELSEIF 서 == 1
                    RETURN 1
                ELSEIF 북 == 1
                    RETURN 3
                ENDIF
            CASE 2
                IF 서 == 1
                    RETURN 1
                ELSEIF 남 == 1
                    RETURN 2
                ELSEIF 북 == 1
                    RETURN 3
                ENDIF
        ENDSELECT
    ENDIF
ELSEIF CP:ARG:1 - 1 == LASTCP:ARG:0
    ;서쪽에서 옴
    IF 이동가능수 < 2
        RETURN 1
    ELSE
        SELECTCASE RAND:3
            CASE 0
                IF 북 == 1
                    RETURN 3
                ELSEIF 남 == 1
                    RETURN 2
                ELSEIF 동 == 1
                    RETURN 0
                ENDIF
            CASE 1
                IF 남 == 1
                    RETURN 2
                ELSEIF 동 == 1
                    RETURN 0
                ELSEIF 북 == 1
                    RETURN 3
                ENDIF
            CASE 2
                IF 동 == 1
                    RETURN 0
                ELSEIF 남 == 1
                    RETURN 2
                ELSEIF 북 == 1
                    RETURN 3
                ENDIF
        ENDSELECT
    ENDIF
ELSEIF (CP:ARG:2) + 1 == LASTCP:ARG:1
    ;남쪽에서 옴
    IF 이동가능수 < 2
        RETURN 2
    ELSE
        SELECTCASE RAND:3
            CASE 0
                IF 북 == 1
                    RETURN 3
                ELSEIF 동 == 1
                    RETURN 0
                ELSEIF 서 == 1
                    RETURN 1
                ENDIF
            CASE 1
                IF 동 == 1
                    RETURN 0
                ELSEIF 서 == 1
                    RETURN 1
                ELSEIF 북 == 1
                    RETURN 3
                ENDIF
            CASE 2
                IF 서 == 1
                    RETURN 1
                ELSEIF 동 == 1
                    RETURN 0
                ELSEIF 북 == 1
                    RETURN 3
                ENDIF
        ENDSELECT
    ENDIF
ELSEIF (CP:ARG:2) - 1 == LASTCP:ARG:1
    ;북쪽에서 옴
    IF 이동가능수 < 2
        RETURN 3
    ELSE
        SELECTCASE RAND:3
            CASE 0
                IF 동 == 1
                    RETURN 0
                ELSEIF 남 == 1
                    RETURN 2
                ELSEIF 서 == 1
                    RETURN 1
                ENDIF
            CASE 1
                IF 남 == 1
                    RETURN 2
                ELSEIF 서 == 1
                    RETURN 1
                ELSEIF 동 == 1
                    RETURN 0
                ENDIF
            CASE 2
                IF 서 == 1
                    RETURN 1
                ELSEIF 남 == 1
                    RETURN 2
                ELSEIF 동 == 1
                    RETURN 0
                ENDIF
        ENDSELECT
    ENDIF
ENDIF