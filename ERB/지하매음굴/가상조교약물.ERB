@D_COM101
#DIM DYNAMIC 강도
;해당하는 약 효과를 넣으세요
IF GETBIT(가약물효과,1) == 0
    ;감소량을 입력하세요
    가체력감소량 = 100
    가기력감소량 = 300
    ;대사를 입력하세요
    PRINTFORML 잠시후 주사기가 담긴 쟁반을 든 직원이 들어와 %가타겟의% 팔에 주사하였다...
    SETCOLOR 0xFFFF00
    PRINTFORML %가타겟은% 참기 어려운 욕망을 느끼기 시작했다... 
    ;소스처리를 하세요
    강도 += 2500
    SETBIT 가약물효과, 1
    ;처리된 소스를 입력하세요
    CALL V_약물(강도)
    CALL V_욕정(강도*2)
    CALL V_반감(강도/2)
    RETURN 1
ELSE
    RETURN 0
ENDIF

