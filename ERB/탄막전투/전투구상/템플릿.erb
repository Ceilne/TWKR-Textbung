[SKIPSTART]
;스킵 스타트랑 엔드는 지워주세요

;RETURN 하는 값이 1이상 이면 이벤트 발생, 0이면 이벤트 취소입니다

;일단은 가볍게 몇개만 넣어둠, 나중에 더 추가될지도 몰라

;파일 제목은 "캐릭터번호 캐릭터이름". 즉, 레이무의 경우 001 레이무.ERB인 셈
@T_KOJO_{캐릭터번호}(ARG,ARG:1)
;PRINTFORML 「」
SELECTCASE ARG
    CASE TK_존재판정
        ;실장된 이벤트의 경우 리턴값을 1로 해주세요
        IF ARG:1 == TK_처형직전
            RETURN 0
        ELSEIF ARG:1 == TK_패배강간직전
            RETURN 0
        ELSEIF ARG:1 == TK_패배강간시작
            RETURN 0
        ENDIF
        ;이건 건들지 말고
        RETURN 0
    CASE TK_처형직전
        ;처형 직전에 살려달라고 애원하는 이벤트
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        CALL ASK_YN
        IF 1
            ;살린다
            RETURN 1
        ELSE
            ;죽인다
            RETURN 0
        ENDIF
    CASE TK_패배강간직전
        ;하지 말아달라는 애원의 이벤트
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        IF 1
            ;멈춘다
            RETURN 1
        ELSE
            ;범한다
            RETURN 0
        ENDIF
    CASE TK_패배강간시작
        ;그런거 없고 당하기 시작하는 시점, 여기서 리턴값 1을 하면 도망가버렸다던가 그런류겠지?
        PRINTFORML 여기에 대사 입력
        PRINTFORML 「」
        IF 1
            ;멈춘다
            RETURN 1
        ELSE
            ;범한다
            RETURN 0
        ENDIF
ENDSELECT
[SKIPEND]