@T_KOJO_61(ARG,ARG:1)
;PRINTFORML 「」
SELECTCASE ARG
    CASE TK_존재판정
        ;실장된 이벤트의 경우 리턴값을 1로 해주세요
        IF ARG:1 == TK_처형직전
            RETURN 0
        ELSEIF ARG:1 == TK_패배강간직전
            RETURN 0
        ELSEIF ARG:1 == TK_패배강간시작
            RETURN 0
        ENDIF
        ;이건 건들지 말고
        RETURN 0
    CASE TK_처형직전
        ;처형 직전에 살려달라고 애원하는 이벤트
        PRINTFORML 「아, 져버렸나」
        PRINTFORML 「미리 말해두겠지만, 나를 죽여봐야 헛수고다」
        PRINTFORML 「괜히 힘빼지 않는게 좋아」
        PRINTFORML 모코우는 지쳤는지 멍하니 하늘을 바라볼 뿐이었다
        PRINTFORML 정말로 죽일까?
        CALL ASK_YN
        IF RESULT
            RETURN 1
        ELSE
            RETURN 0
        ENDIF
    CASE TK_패배강간직전
        ;하지 말아달라는 애원의 이벤트
        PRINTFORML 「그럴 목적이라면, 차라리 죽여라」
        PRINTFORML 「기분 더러우니깐」

        PRINTL 모코우는 더럽다는듯이 노려보고 있다...
        PRINTL 정말로 범할까?
        CALL ASK_YN
        IF RESULT
            RETURN 1
        ELSE
            RETURN 0
        ENDIF
    CASE TK_패배강간시작
        ;그런거 없고 당하기 시작하는 시점, 여기서 리턴값 1을 하면 도망가버렸다던가 그런류겠지?
        PRINTFORML 「큿, 이런 쓰레기 자식!」
        PRINTFORML 「그 더러운 손으로 만지지 마!」
        IF 0
            RETURN 1
        ELSE
            RETURN 0
        ENDIF
ENDSELECT