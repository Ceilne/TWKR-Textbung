@타겟TEQUIP(ARG)
#FUNCTION
FOR LOCAL, 0, GET_TARGETNUM()
    IF !TARGET:LOCAL
        CONTINUE
    ENDIF
    IF TEQUIP:(TARGET:LOCAL):ARG
        RETURNF 1
    ENDIF
NEXT
RETURNF 0

@몸사용하기(ARGS,ARG)

SELECTCASE ARGS
    CASE "입"
        몸사용중:GB_입:GB_사용정도 = ARG
        몸사용중:GB_입:GB_사용자++
    CASE "손"
        몸사용중:GB_손:GB_사용정도 = ARG
        몸사용중:GB_손:GB_사용자++
    CASE "C"
        몸사용중:GB_C:GB_사용정도 = ARG
        몸사용중:GB_C:GB_사용자++
    CASE "V"
        몸사용중:GB_V:GB_사용정도 = ARG
        몸사용중:GB_V:GB_사용자++
    CASE "A"
        몸사용중:GB_A:GB_사용정도 = ARG
        몸사용중:GB_A:GB_사용자++
    CASE "B"
        몸사용중:GB_B:GB_사용정도 = ARG
        몸사용중:GB_B:GB_사용자++
    CASE "P삽입"
        몸사용중:GB_P삽입:GB_사용정도 = ARG
        몸사용중:GB_P삽입:GB_사용자++
ENDSELECT